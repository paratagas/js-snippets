{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "lib/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 90,
      "branches": 85,
      "functions": 90,
      "lines": 90
    }
  },
  "notify": true,
  "notifyMode": "failure",
  "testRegex": "test/.*\\.test\\.js$"
}
